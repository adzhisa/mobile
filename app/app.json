{
  "expo": {
    "name": "Mobile",
    "slug": "Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/drawable/icon.png", 
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#410FA3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/drawable/icon.png",
        "backgroundColor": "#410FA3"
      }
    },
    "web": {
      "favicon": "./src/drawable/icon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
